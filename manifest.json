{
    "manifest_version": 2,
    "name": "Fine Print",
    "short_name": "Fine Print",
    "description": "Chrome Extension for Improving Printed Version of Popular Online Websites",
    "version": "1.0.0",
    "icons": {
        "128": "icon/default.png"
    },
    "background": {
        "scripts": [ 
            "page_rules.js",
            "validator.js",
            "background.js"
        ],
        "persistent": false
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "web_accessible_resources": [
        "css/*"
    ],
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [ "<all_urls>" ],
            "js": [
                "injector.js"
            ]
        }
    ],
    "permissions": []
}